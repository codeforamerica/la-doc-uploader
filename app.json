{
  "name": "LA document uploader review app",
  "description": "Heroku review apps from GitHub PRs",
  "formation": {
    "web": {
      "quantity": 1
    }
  },
  "stack": "heroku-22",
  "buildpacks": [
    {
      "url": "heroku/jvm"
    },
    {
      "url": "heroku/gradle"
    },
    {
      "url": "heroku/java"
    }
  ],
  "env": {
    "LOG_LEVEL": {
      "value": "info"
    },
    "HEROKU_APP_NAME": {
      "required": true
    }
  },
  "addons": [
    "heroku-postgresql"
  ]
}
